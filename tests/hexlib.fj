// if
def print_if hex @ l0, l1 < IO {
    hex.if hex, l0, l1
    l0: IO+0;l1+dw
    l1: output '1'
}
def print_if0 hex @ l0 < IO {
    hex.if0 hex, l0
    IO+1;l0+dw
    l0: output '0'
}
def print_if1 hex @ l1 < IO {
    hex.if1 hex, l1
    IO+0;l1+dw
    l1: output '1'
}
