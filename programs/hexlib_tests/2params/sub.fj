stl.startup

rep(256, i) test_sub i&0xf, i>>4

stl.loop


hex.init


def test_sub a, b @ c0_1st, c1_1st, print_1st, c0_2nd, c1_2nd, print_2nd, ah_1st, bh, ah_2nd, end < hex.sub.dst {
    hex.sub.clear_carry
    hex.sub ah_1st, bh
    hex.sub.clear_carry c0_1st, c1_1st
  c0_1st:
    stl.output '0'
    ;print_1st
  c1_1st:
    stl.output 'f'
  print_1st:
    hex.print_as_digit ah_1st, 0
    stl.output '-'

    hex.sub.not_carry
    hex.sub ah_2nd, bh
    hex.sub.clear_carry c0_2nd, c1_2nd
  c0_2nd:
    stl.output '0'
    ;print_2nd
  c1_2nd:
    stl.output 'f'
  print_2nd:
    hex.print_as_digit ah_2nd, 0
    stl.output '\n'
    ;end

  ah_1st: hex.hex a
  ah_2nd: hex.hex a
  bh:     hex.hex b
  end:
}
