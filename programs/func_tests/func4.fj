stl.startup_and_init_all 10


// Prints "ABCDEFGH-" and then 0/1, the invert of x4
test4:
    stl.output 'A'
    hex.push x4
    stl.output 'B'

    stl.call func4

    stl.output 'G'
    hex.pop x4
    stl.output 'H'
    bit.bin2ascii ascii, x4
    stl.output '-'
    bit.print ascii

    stl.output '\n'
    stl.loop

func4:
    stl.output 'C'
    stl.get_sp __func4_arg_ptr
    stl.output 'D'
    hex.dec_ptr __func4_arg_ptr
    stl.output 'E'
    hex.ptr_flip_dbit __func4_arg_ptr
    stl.output 'F'
    stl.return
  __func4_arg_ptr:
    hex.vec w/4, 0



x4:
    bit.bit 0

ascii:
    bit.vec 8
