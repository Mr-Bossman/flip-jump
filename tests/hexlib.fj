// if n
def print_if_16 hex @ l0, l1 < IO {
    hex.if 16, hex, l0, l1
    l0: IO+0;l1+dw
    l1: output '1'
}
def print_if0_16 hex @ l0 < IO {
    hex.if0 16, hex, l0
    IO+1;l0+dw
    l0: output '0'
}
def print_if1_16 hex @ l1 < IO {
    hex.if1 16, hex, l1
    IO+0;l1+dw
    l1: output '1'
}
