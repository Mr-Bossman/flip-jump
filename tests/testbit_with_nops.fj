.startup

.rep 10 i mov y z
.mov x x
.if x l0 l1

l0:
    .output 0x5A // 'Z'
    ;loop
l1:
    .output 0x10*3+1*3-4/2 // '1'
    ;
    temp;loop
loop:
    temp;
    temp
    .loop
    ..pad 2
    ..flip_by temp loop
    ..flip_by_dbit temp loop

    [1]1
    dloop:;dloop
    .var 8 3
    [5]3
    bananas:
    .output 0x5c
    dy=3

    .rep 7 i
    .end
    n=15
    .rep 8 q
        .if x l0 l1
    .end
    bans2:

    .rep n j
        .temp_macro x x
    .end

.def temp_macro g y
    .not g
    .not y
.end

x: .bit 1   // bit0 => 'Z',  bit1 => '1'
y: .bit 1
z: .bit 1

