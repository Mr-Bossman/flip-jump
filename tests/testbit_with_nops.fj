.startup
.safe_mov x x
.if x l0 l1

l0:
    ..output 5A // 'Z'
    ;loop
l1:
    ..output 31 // '1'
    ;
    temp;loop
loop:
    temp;
    temp
    cloop:;cloop
    ..pad 2
    ..flip_by temp loop
    ..flip_by_dbit temp loop

    1#3
    dloop:;dloop
    ..var 8 3
    5#3
    bananas:
    ..output 5c
    dy=3

    .rep 7 i
    .end
    n=15
    .rep 8 q
        .if x l0 l1
    .end
    bans2:

    .rep n j
        .temp_macro x zero
    .end

.def temp_macro g y
    .not g
    .not y
.end

x:
    .bit0   // bit0 => 'Z',  bit1 => '1'
