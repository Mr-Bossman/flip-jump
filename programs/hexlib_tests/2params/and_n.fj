startup

AND_N = 0x0000ffff8000b32cccc69dea2047c8e0ae1e5299
rep(100, i) test_and_n 4, (AND_N>>((i%10)*4)&0xf), (AND_N>>((i/10))*4&0xf)

loop


hex.init


def test_and_n n, a, b @ lt, eq, gt, ah, bh, ch, end {
    hex.and n, ah, bh
    hex.cmp n, ah, ch, lt, eq, gt
    
  lt:
    output '<'
    ;end
  eq:
    output '='
    ;end
  gt:
    output '>'
    ;end
    
  ah: hex.vec n, a
  bh: hex.vec n, b
  ch: hex.vec n, a&b
  end:
    output '\n'
}
